                    var url = '@Url.Action("ShowPdf", "App")';
                    window.open(url);
                });
            });

            //$('a[href="/Job/JobsIndex"]').parent().addClass('active');
        });

        function ShowProgress() {
            setTimeout(function () {
                var modal = $('<div id="mod" />');
                $('body').append(modal);
                var loading = $(".loading");
                loading.show();
                var top = Math.max($(window).height() / 2 - loading[0].offsetHeight / 2, 0);
                var left = Math.max($(window).width() / 2 - loading[0].offsetWidth / 2, 0);
                loading.css({ top: top, left: left });
            }, 200);
        }
        function DisplayNotification(returnvalue, status) {
            if (returnvalue != null) {
                if (status == "success") {
                    toastr.options = { "positionClass": "toast-bottom-right" };
                    toastr.success(returnvalue);
                }
                else if (status == "error") {
                    toastr.options = { "positionClass": "toast-bottom-right" };
                    toastr.error(returnvalue);

                }
            }
        }

        function HideNotification() {
            toastr.hide;
        }
        function Expend()
        {

            var className = $('#Expend').attr('class');
            if (className == "glyphicon glyphicon-plus")
            {
                $('#Expend').removeClass("glyphicon glyphicon-plus");
                $('#Expend').addClass("glyphicon glyphicon-minus");
            }
            else if (className == "glyphicon-plus glyphicon collapsed")
            {
                $('#Expend').removeClass("glyphicon glyphicon-plus");
                $('#Expend').addClass("glyphicon glyphicon-minus");
            }
            else if(className=="glyphicon glyphicon-plus collapsed")
            {
                $('#Expend').removeClass("glyphicon glyphicon-plus");
                $('#Expend').addClass("glyphicon glyphicon-minus");
            }
            else if(className=="glyphicon-plus glyphicon")
            {
                $('#Expend').removeClass("glyphicon glyphicon-plus");
                $('#Expend').addClass("glyphicon glyphicon-minus");
            }
            else
            {
                 $('#Expend').removeClass("glyphicon glyphicon-minus");
                 $('#Expend').addClass("glyphicon glyphicon-plus");
            }
        }
        function DistExpend() {

            var className = $('#DistExpend').attr('class');
            if (className == "glyphicon glyphicon-plus") {
                $('#DistExpend').removeClass("glyphicon glyphicon-plus");
                $('#DistExpend').addClass("glyphicon glyphicon-minus");
            }
            else if (className == "glyphicon-plus glyphicon collapsed") {
                $('#DistExpend').removeClass("glyphicon glyphicon-plus");
